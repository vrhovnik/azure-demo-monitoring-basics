@model List<Monitoring.General.ViewModels.CustomLogViewModel>
@if (Model.Any())
{
    <table class="table table-hover">
        <thead>
        <tr>
            <th scope="col">Name</th>
            <th scope="col">Counter name</th>
            <th scope="col">Counter value</th>
        </tr>
        </thead>
        <tbody>
        @foreach (var currentLogData in Model)
        {
            <td>@currentLogData.Name</td>
            <td>@currentLogData.CounterName</td>
            <td>@currentLogData.CounterValue</td>
        }
        </tbody>
    </table>
}
else
{
    <p>No data has been retrieved, check logs for more details.</p>
}
